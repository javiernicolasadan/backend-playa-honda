{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/api/sitemap.xml"   
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"  
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"   
    }
  ]
}
